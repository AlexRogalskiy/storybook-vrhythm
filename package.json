{
  "name": "storybook-vrhythm",
  "version": "0.0.4",
  "description": "Storybook Vertical Rhythm Decorator",
  "homepage": "https://github.com/jmlweb/storybook-vrhythm",
  "main": "dist/index.js",
  "umd:main": "dist/storybook-vrhythm.umd.production.js",
  "module": "dist/storybook-vrhythm.es.production.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react": "^16.12.0",
    "tsdx": "^0.12.3",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@storybook/addons": "5.3.13"
  }
}
